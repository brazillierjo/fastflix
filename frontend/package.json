{
  "name": "fastflix",
  "main": "expo-router/entry",
  "version": "2.1.6",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint --max-warnings 0",
    "lint:warnings": "expo lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major",
    "version:status": "node scripts/version-bump.js status",
    "build:ios": "node scripts/version-bump.js build-ios",
    "build:android": "node scripts/version-bump.js build-android",
    "build:both": "node scripts/version-bump.js build-both",
    "typecheck": "tsc --noEmit",
    "test": "jest --forceExit 2>&1 | grep -Ev 'Force exiting Jest|worker process has failed to exit gracefully|--detectOpenHandles|Active timers' || true",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit 2>&1 | grep -Ev 'Force exiting Jest|worker process has failed|--detectOpenHandles|Active timers' || true",
    "test:ci": "jest --ci --coverage --watchAll=false --forceExit 2>&1 | grep -Ev 'Force exiting Jest|worker process has failed|--detectOpenHandles|Active timers' || true",
    "pre-commit": "npm run lint && npm run format:check && npm run typecheck && npm run test:ci",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "^7.7.0",
    "@tanstack/react-query": "^5.80.6",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "expo": "~53.0.10",
    "expo-apple-authentication": "~7.2.4",
    "expo-auth-session": "^7.0.9",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.0",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~7.1.5",
    "expo-localization": "^16.1.5",
    "expo-router": "~5.0.7",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.1.6",
    "moti": "^0.30.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-purchases": "^8.11.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "expo-application": "~6.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "knip": "^5.70.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
