# BACKEND API URL
# Development (local backend)
# EXPO_PUBLIC_API_URL=http://localhost:3000
# Production (Vercel)
EXPO_PUBLIC_API_URL=https://fastflix-api.vercel.app

# REVENUECAT API KEYS (for subscriptions)
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=your_revenuecat_ios_api_key_here
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY=your_revenuecat_android_api_key_here

# GOOGLE SIGN IN CLIENT IDS
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id.apps.googleusercontent.com

# BUILD CONFIGURATION
# iOS Build Number (increment for each App Store submission)
IOS_BUILD_NUMBER=1

# Android Version Code (increment for each Play Store submission)
ANDROID_VERSION_CODE=1
